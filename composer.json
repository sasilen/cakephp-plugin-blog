{
    "name": "sasilen/blog",
    "description": "Blog plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "^3.5",
        "cakedc/users": "^5.0",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-google": "^2.0",
	"league/oauth1-client": "@stable",
        "romano83/cakephp3-media": "dev-master",
        "romano83/cakephp3-draft": "1.*",
        "mirko-pagliai/cakephp-thumber": "^1.0",
        "kareylo/cakephp-comments": "^0.0.4",
        "dereuromark/cakephp-ratings": "dev-master",
        "muffin/tags": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Blog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blog\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
