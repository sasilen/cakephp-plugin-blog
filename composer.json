{
    "name": "sasilen/blog",
    "description": "Blog plugin for CakePHP 4",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "~4.1.0",
        "muffin/tags": "2.0.0-beta",
        "davidyell/proffer": "dev-cake-4",
        "mirko-pagliai/cakephp-thumber" : "^1.9"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sasilen\\Blog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sasilen\\Blog\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
